<?xml version="1.0" encoding="utf-8"?><layer-list xmlns:android="http://schemas.android.com/apk/res/android">    <item android:id="@android:id/background">        <shape>            <corners android:radius="2dip"/>            <gradient                android:angle="0"                android:centerColor="#d1d1d1"                android:centerY="0.75"                android:endColor="#d1d1d1"                android:startColor="#d1d1d1"/>        </shape>    </item>    <item android:id="@android:id/secondaryProgress">        <clip>            <shape>                <corners android:radius="2dip"/>                <gradient                    android:angle="0"                    android:centerColor="#474a4a"                    android:centerY="0.75"                    android:endColor="#474a4a"                    android:startColor="#474a4a"/>            </shape>        </clip>    </item>    <item android:id="@android:id/progress">        <clip>            <shape>                <corners android:radius="2dip"/>                <gradient                    android:angle="0"                    android:endColor="#474a4a"                    android:startColor="#474a4a"/>            </shape>        </clip>    </item></layer-list>